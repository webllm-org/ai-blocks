{
  "name": "ai-blocks",
  "version": "0.0.1",
  "private": false,
  "description": "AI-focused UI components for building LLM-powered interfaces with WebLLM",
  "homepage": "https://webllm.org/blocks",
  "repository": {
    "type": "git",
    "url": "https://github.com/webllm-org/webllm",
    "directory": "packages/ai-blocks"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./demos/*": {
      "import": "./dist/demos/*.js"
    },
    "./registry/*": "./src/registry/*",
    "./lib/*": "./src/lib/*"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "generate:registry": "node scripts/generate-registry-from-blocks.js",
    "build:registry": "node scripts/generate-registry-from-blocks.js && node scripts/build-registry.js",
    "dev": "node scripts/build-registry.js --watch",
    "clean": "rm -rf dist ../playground/public/r",
    "extract:props": "node scripts/extract-props.cjs"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "peerDependencies": {
    "lucide-react": ">=0.300.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "webllm": "*"
  },
  "peerDependenciesMeta": {
    "webllm": {
      "optional": true
    },
    "lucide-react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "react-docgen-typescript": "^2.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  }
}
