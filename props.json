{
  "ui/ai-avatar": {
    "componentName": "AIAvatar",
    "description": "",
    "props": [
      {
        "name": "src",
        "type": "string",
        "required": false,
        "description": "Avatar image source"
      },
      {
        "name": "alt",
        "type": "string",
        "required": false,
        "defaultValue": "Avatar",
        "description": "Alt text for the avatar image"
      },
      {
        "name": "fallback",
        "type": "string",
        "required": false,
        "description": "Fallback text when no image (typically initials)"
